   function  DealAsItem(hl,sItemText)
   {
      if(sItemText=="")return;
      var iPos = sItemText.indexOf("※");
      if(iPos!=-1)
      {
          var iType = sItemText.substr(0,iPos);
          var sContent = sItemText.substr(iPos+1);
          var sItemToCreate = "";
          switch(iType)
          {
              case "0":
              case "1":
              case "3":
              case "6":
              {
                 sItemToCreate = "a";
                 var vBr = document.createElement("BR");
                 var vItem = document.createElement(sItemToCreate);
                 vItem.innerText = sContent;
                 hl.appendChild(vBr);
                 hl.appendChild(vItem);
              }
              break;
              case "2":
              {
                var vBr = document.createElement("BR");
                var vButton = document.createElement("input type=\"text\" disabled = false >");
                vButton.value = sContent;
                hl.appendChild(vBr);
                hl.appendChild(vButton);

              }
              break;
              case "4":
              {
                 sItemToCreate = "input";
                 var vBr = document.createElement("BR");
                 var vText = document.createElement("a");
                 var vButton = document.createElement("<input type=radio disabled = false>");
                 vText.innerText = sContent;
                 hl.appendChild(vBr);
                 hl.appendChild(vButton);
                 hl.appendChild(vText);
              }
              break;
              case "5":
              {
                 sItemToCreate = "input";
                 var vBr = document.createElement("BR");
                 var vText = document.createElement("a");
                 var vButton = document.createElement("<input type=checkbox disabled = false>");
                 vText.innerText = sContent;
                 hl.appendChild(vBr);
                 hl.appendChild(vButton);
                 hl.appendChild(vText);
                 
              }
              break;
          }
      }
   }
   
