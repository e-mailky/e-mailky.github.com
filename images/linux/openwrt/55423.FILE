    function HideBtnNearBy(dlItem)
    {
          var labels = dlItem.childNodes;        //获取ID为Label无素下的所有子节点，childNodes是DOM的一个属性 
          //对所有子节点进行循环，增加onmouseover事件，也可以根据需要添加onclick事件    
          for(var iLoop = 0; iLoop < labels.length; iLoop ++)    
          {    
              var label = labels[iLoop];    
              if ("BeShowAndHide" == label.name)   //只处理LI的DOM节点，目的是为了兼容Firefox，因为Firefox会把空格与换行与当成一个节点处理    
              {
                  label.style.visibility="hidden";
                  return true;
              }
              if(HideBtnNearBy(label))
                 return true;
          }    
          return false;
    }
    
    
