    <title>无标题页</title>
    <script type="text/javascript">
    
    function SetSelectBk(obj)
    {
        var a = document.getElementsByTagName("dl");
        for(var i=0;i<a.length;i++){
        a[i].className="div3";
        }
        obj.className="div5";
    }
    
    var obTemp;
    function SetOverBk(obj)
    {
        var a = obj.parentNode;
        if(a.className!="div5")
            a.className="div4";
    }
    function SetLeaveBk(obj)
    {
        var a = obj.parentNode;
        if(a.className!="div5")
            a.className="div3";
    }
    
    
    
        var bStrScroll  =  "Init String";
        var sToThisMsg  = "Init String";
        var bIsSearch        = false;
       //等比例显示图片
        function DrawImage(ImgD)
        {
         var image=new Image();
         image.src=ImgD.src;
         if(image.width>0 && image.height>0){
         if(image.width/image.height>= 260/300){
          if(image.width>260){
          ImgD.width=260;
          ImgD.height=(image.height*260)/image.width;
          }else{
          ImgD.width=image.width;
          ImgD.height=image.height;
          }
          }
         else{
          if(image.height>300){
          ImgD.height=300;
          ImgD.width=(image.width*300)/image.height;
          }else{
          ImgD.width=image.width;
          ImgD.height=image.height;
          }
          }
         }
         delete image;
         image = null;
         
         CxCalScroll();
         
          var iPos = bStrScroll.indexOf(ImgD.src);
          if(iPos==-1)
          {
             if(sToThisMsg=="Init String")
              {
                 if(!bIsSearch)
                 {
                     CxScrollPos(-1);
                 }
                  bStrScroll = bStrScroll+"*"+ImgD.src;
              }
              else
              {
                  ControlWeb(sToThisMsg,"ScrollToAnthor");
                  bStrScroll = bStrScroll+"*"+ImgD.src;
                  
              }
          }
        }
    
 //******************************************************/
        var mySetTimeOut = setTimeout; 
        window.setTimeout = function(callback, timeout)
        { 
            var args = Array.prototype.slice.call(arguments, 2); 

            function callFn()
            {
               callback.apply(null, args);
             }
            return mySetTimeOut(callFn, timeout); 
       }
    //新加判断单机和双击事件
    var clickRepeat;
    var clickPicRepeat;
    var clickPicCount=0;
    var clickCount=0;
    var clickDelay=300;
    function checkPicClick(obj) 
    {   
        clearTimeout(clickPicRepeat);
        clickPicCount++;
        if(clickPicCount==1) 
          {          
             clickPicRepeat=window.setTimeout(ClickPic,clickDelay,obj); 
          }
         else 
          {          
             open_pic(obj);
             clickPicCount=0;
          }
    }
     function voiceclick(cObject) 
   {
       HisCallBack.File_Interface("ClickPlayVoice",cObject.sName,cObject.fObject,cObject.fPath,cObject.className,cObject.id,cObject.src,cObject.name);
       
               var childs = cObject.childNodes;         for(var i = childs.length - 1; i >= 0; i--)         {             if (childs[i].id == "playUnread")             {                cObject.removeChild(childs[i]);             }        } } 
    function ShowMessage(btnShow)
    {
       //window.external.IWEB_ShowNearByMessage(btnShow.udata);
        HisCallBack.File_Interface("ClickNearMsg",btnShow.udata);
    }
    
   function fileclick(cObject)
   {
       // window.external.IWEB_ClickFileBtn(cObject.fPath, cObject.fObject,cObject.sName);
       HisCallBack.File_Interface("ClickFile",cObject.sName, cObject.fObject,cObject.fPath);
   }
    
    function appmsgclick(cObject)
    {
       // window.external.IWEB_ClickAppMsgBtn(cObject.sBtnId, cObject.sBtnName,cObject.sMsgId);
       HisCallBack.File_Interface("ClickAppMsg",cObject.sMsgId,cObject.sBtnName,cObject.sBtnId);
    }
          //重新加载
      function CxReload()
      {
        $("#content").html("");
        bStrScroll  =  "Init String";
        sToThisMsg  = "Init String";
        bIsSearch   = false;
      }
    
    function ShowBtnNearBy(dlItem)
    {
          var labels = dlItem.childNodes;        //获取ID为Label无素下的所有子节点，childNodes是DOM的一个属性 
          //对所有子节点进行循环，增加onmouseover事件，也可以根据需要添加onclick事件    
          for(var iLoop = 0; iLoop < labels.length; iLoop ++)    
          {    
              var label = labels[iLoop];    
