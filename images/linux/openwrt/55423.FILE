$.boxScroll = (function() {
	var config = {
		cursorcolor:"#d1d1d1",  
		autohidemode : "false", 
		cursoropacitymax:0.5,   
		cursorwidth:"10px",  
		cursorborder:"0",  
		cursorborderradius:"3px",
		background : "#eee",
		horizrailenabled:false,
		boxzoom : false
	}
	
	return {
		show : function( obt,ops ) {
			if( ops ) {
				for( var i in ops ) {
					if( config[ i ] ) {
						config[ i ] = ops[i];
					}
				}
			}
			$( obt ).niceScroll(config);
		},
		resize : function( obt ) {
			$( obt ).getNiceScroll().resize();	
		}
	}
})();




