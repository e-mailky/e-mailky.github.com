<style type="text/css">
body{ padding:0px 0px 0px 0px;margin:0px 0px 0px 0px}
#content {margin:0px 0px 0px 0px;padding:5px 8px 0px 10px ;}
.key {color:#f00;background:#ff4}
.div1{width:98%; height:100px; font:9pt , Helvetica, sans-serif; color:#f00; border:1pt solid #f00;word-wrap:break-word;}
.div2{width:98%; height:100px; font:9pt Microsoft YaHei , Helvetica, sans-serif; color:#00f; border:1pt solid #00f;word-wrap:break-word;}
.div3{width:98%; font:9pt Microsoft YaHei , Helvetica, sans-serif; color:#000; border:1pt solid #f6f8f9;word-wrap:break-word;}
.div4{width:98%; font:9pt Microsoft YaHei , Helvetica, sans-serif; color:#333; border:1pt solid #afd9e9;background:#eaf6fe;word-wrap:break-word;}
.div5{width:98%; font:9pt Microsoft YaHei , Helvetica, sans-serif; color:#333; border:1pt solid #FFDB93;background:#ffedc4;word-wrap:break-word;}
.fnormal{font:9pt Microsoft YaHei , Helvetica, sans-serif; color:#f00;word-wrap:break-word;}
.do{width:98%;background:#FFFFFF;border:1pt solid #f6f8f9}
.doo{width:98%;background:#FFF37B;border:1pt solid #f6f8f9}
 a:link {color:#00a0e9;}
 a:visited {color:#00a0e9;}
 a:hover {color:#00a0e9;}
 a:active {color:#00a0e9;}
 .bt{width:80%;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;margin-top:-16px;margin-left:20px;}
 
#tooltipObj{
	z-index:1000000;text-align:left;
}
#tooltipObj div{
	position:relative;
}
#tooltipObj .tooltip_arrow{
	background-image:url('images/arrow.gif');
	width:10px;position:absolute;
	left:0px;top:0px;
	background-repeat:no-repeat;
	background-position:center left;
	z-index:1000005;height:60px;
}
#tooltipObj .tooltip_content{
	border:1px solid #666666;
	left:6px;top:0px;
	position:absolute;width:240px;
	background-color:#eaf6fe;
	font-size:0.8em;
	overflow:hidden;z-index:1000001;
	font:9pt Microsoft YaHei , Helvetica, sans-serif;color:#333333;
}
dt{margin-left:0;height:12pt;padding-top:2pt;padding-bottom:2pt;}
dd{margin-left:0;margin-left: 20px;}

.nav {
    MARGIN: 1pt 0; 
    WIDTH: 100%; 
    FONT-FAMILY: verdana; 
    HEIGHT: 21pt; 
    BACKGROUND-COLOR: #970B0B;
    font-family:Microsoft YaHei , Helvetica, sans-serif;
    font-size:9pt;
}
.nav UL {
    PADDING: 0px; 
    DISPLAY: block; 
    MARGIN: 0px; 
    LIST-STYLE-TYPE: none; 
    HEIGHT: 21pt; 
    BACKGROUND-COLOR: #C3E7FF;
    COLOR: #ffffff; 
}
.nav LI {
    BORDER-RIGHT: #ffffff 1px solid; 
    DISPLAY: block; 
    FLOAT: left; 
    HEIGHT: 21px;
    font-family:Microsoft YaHei , Helvetica, sans-serif;
    font-size:9pt;
}
.nav LI A {
    PADDING:1pt 15pt 0; 
    DISPLAY: block; 
    FONT-WEIGHT: none; 
    COLOR: #ffffff; 
    LINE-HEIGHT: 20px; 
    TEXT-DECORATION: none;
}
.nav LI A:hover {
    COLOR:#FFFFFF; 
    BACKGROUND-COLOR: #5FBFFF; 
    TEXT-DECORATION: none;
}
.current{
  color:#ffffff;
  background:#0099FF;
}


::selection{background:#3397FF;color:#FFF;}
::-moz-selection{background:#3397FF;color:#FFF;}

</style>

